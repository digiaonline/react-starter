{
  "name": "nord-react-starter",
  "version": "1.0.0",
  "description": "React starter template from Nord Software.",
  "main": "src/main.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --colors --history-api-fallback --config webpack/development.js",
